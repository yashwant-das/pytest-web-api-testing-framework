{
    "base_web_url": "https://www.saucedemo.com",
    "base_api_url": "https://restful-booker.herokuapp.com",
    "browser": "chrome",
    "headless": false,
    "default_timeout": 10,
    "login_path": "/",
    "home_path_indicator": "inventory.html",
    "api_auth_endpoint": "/auth",
    "credentials": {
        "standard_user": {
            "username_env": "SAUCE_USERNAME",
            "password_env": "SAUCE_PASSWORD"
        },
        "api_user": {
            "username_env": "BOOKER_USERNAME",
            "password_env": "BOOKER_PASSWORD"
        }
    }
}
